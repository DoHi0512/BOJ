#include <iostream>
#include <queue>
using namespace std;
int main() {
	int n,last=0,temp;
	cin >> n;
	queue <int> que;
	for (int i = 1; i <= n; i++) {
		que.push(i);
	}
	for(int i=0;i<n;i++) {
		last = que.front();
		que.pop();
		temp = que.front();
		que.pop();
		que.push(temp);
	}
	cout << last;
}
